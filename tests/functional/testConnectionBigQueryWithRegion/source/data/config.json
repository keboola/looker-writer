{
  "action": "testConnection",
  "parameters": {
    "db": {
      "driver": "bigquery",
      "dataset": "%env(string:BIGQUERY_DATASET)%",
      "region": "%env(string:BIGQUERY_REGION)%",
      "service_account": {
        "type": "%env(string:BIGQUERY_SA_TYPE)%",
        "project_id": "%env(string:BIGQUERY_SA_PROJECT_ID)%",
        "private_key_id": "%env(string:BIGQUERY_SA_PRIVATE_KEY_ID)%",
        "#private_key": "%env(string:BIGQUERY_SA_PRIVATE_KEY)%",
        "client_email": "%env(string:BIGQUERY_SA_CLIENT_EMAIL)%",
        "client_id": "%env(string:BIGQUERY_SA_CLIENT_ID)%",
        "auth_uri": "%env(string:BIGQUERY_SA_AUTH_URI)%",
        "token_uri": "%env(string:BIGQUERY_SA_TOKEN_URI)%",
        "auth_provider_x509_cert_url": "%env(string:BIGQUERY_SA_AUTH_X509_CERT_URL)%",
        "client_x509_cert_url": "%env(string:BIGQUERY_SA_CLIENT_X509_CERT_URL)%"
      }
    }
  }
}